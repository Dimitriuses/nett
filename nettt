net="10.7.180."
diapazon=$(echo {127..135})
size_diap=${#diapazon[@]}
if [ -e ./cash.scan ]
then
log=( $(cat ./cash.scan) )
now='='
for i in $diapazon
do
	ip="$net$i"
	ping -c1 $ip &>>/dev/null && now="+" || now="-"
	if [ $now != ${log[$i]} ]
	then
		echo "$net$i $now"
		log[$i]=$now
	fi
done
echo ${log[@]}>cash.scan
else
for i in {0..255}
do
        echo "="
done >>cash.scan
fi
